apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: finalproject-ingress
  namespace: ivolve
  annotations:
    kubernetes.io/ingress.class: "nginx"
spec:
  rules:
  - http:
      paths:
      - path: /
        pathType: Prefix
        backend:
          service:
            name: finalproject-service
            port:
              number: 80

